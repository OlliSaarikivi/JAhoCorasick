<?xml version="1.0"?>
<project name="aho-corasick" basedir="." default="compile">
    <property name="src" value="src"/>
    <property name="output" value="bin"/>

    <target name="compile">
        <javac destdir="bin" includeantruntime="false">
            <src path="${src}"/>
        </javac>
    </target>

    <target name="clean">
        <delete dir="${output}"/>
        <mkdir dir="${output}"/>
    </target>
</project>